<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="com.example.dnd1.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" text="GAME OVER!" />
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Time played:" />
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enemies killed:" />
    <Button onAction="#onHelloButtonClick" text="Quit" />
</VBox>
